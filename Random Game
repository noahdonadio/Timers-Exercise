function randomGame(){
    let num;
    let times = 0;
    let timer = setInterval(function(){
        num = Math.random();
        console.log(num)
        times++
        console.log(times)
        if(num > .75) {
            clearInterval(timer);
            console.log("Number of tries = " + times)
        }
    }, 1000)
}
